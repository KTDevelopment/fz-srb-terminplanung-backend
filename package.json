{
  "name": "fz-srb-terminplanung-backend",
  "version": "3.2.1",
  "description": "backend",
  "author": "Kevin Th√ºrmann",
  "license": "ISC",
  "scripts": {
    "generateMigrations": "typeorm migration:generate -n",
    "prebuild": "rimraf dist && yarn copy:version",
    "build": "nest build -p ./tsconfig.json",
    "build:prod": "yarn build && mv dist/main.js dist/production-server.js && mv dist/main.js.map dist/production-server.js.map",
    "build:staging": "yarn build && mv dist/main.js dist/staging-server.js && mv dist/main.js.map dist/staging-server.js.map",
    "start": "yarn prebuild && nest start -p ./tsconfig.json",
    "start:dev": "yarn prebuild && NODE_ENV=development nest start --watch -p ./tsconfig.json",
    "start:debug": "yarn prebuild && NODE_ENV=development nest start --debug --watch -p ./tsconfig.json",
    "start:staging": "DEPLOYMENT=staging pm2 start dist/staging-server.js && pm2 save",
    "start:prod": "DEPLOYMENT=production pm2 start dist/production-server.js && pm2 save",
    "stop:staging": "pm2 delete staging-server && pm2 save",
    "stop:prod": "pm2 delete production-server && pm2 save",
    "test": "yarn test:unit && yarn test:e2e",
    "test:coverage": "yarn test:unit --collectCoverage",
    "test:unit": "NODE_ENV=test jest",
    "test:e2e": "NODE_ENV=test jest --config test/e2e/setup/jest-e2e.config.js",
    "test:watch": "yarn test:unit --watch",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "copy:version": "mkdir dist && cp -rv src/version.json dist/version.json",
    "preRelease": "gulp increaseVersion",
    "release:staging": "yarn build:staging && yarn test && gulp increaseVersion && yarn package:staging",
    "release:prod": "yarn build:prod && yarn test && gulp increaseVersion && yarn sentry:release && yarn package:prod",
    "sentry:release": "gulp releaseToSentry",
    "sentry:sorucemaps": "gulp sourceMapsToSentry",
    "package:prod": "zip -r prod-release.zip README.md tsconfig.json package.json src config",
    "package:staging": "zip -r staging-release.zip README.md tsconfig.json package.json src config",
    "staging": "yarn install && yarn build:staging && yarn start:staging",
    "prod": "yarn install && yarn build:prod && yarn start:prod"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.10.14",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/schedule": "^0.2.0",
    "@nestjs/serve-static": "^2.0.0",
    "@nestjs/swagger": "^4.2.3",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjsx/crud": "^4.4.1",
    "@nestjsx/crud-typeorm": "^4.4.1",
    "@sentry/integrations": "5.13.0",
    "@sentry/node": "5.12.4",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.1.1",
    "firebase-admin": "^8.9.2",
    "helmet": "^3.21.3",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "nest-raven": "^5.0.0",
    "node-config-ts": "^3.0.3",
    "node-ical": "^0.10.0",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "purify-ts": "^0.14.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@testdeck/jest": "^0.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "chalk": "^3.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "jest": "^24.9.0",
    "jest-stare": "^1.26.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
