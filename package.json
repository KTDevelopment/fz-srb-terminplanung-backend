{
  "name": "fz-srb-terminplanung-backend",
  "version": "3.9.1",
  "description": "backend",
  "author": "Kevin Th√ºrmann",
  "license": "ISC",
  "scripts": {
    "clear": "rm -rf node_modules",
    "generateMigrations": "typeorm migration:generate -n",
    "prebuild": "rimraf dist",
    "build": "nest build -p ./tsconfig.json",
    "build:prod": "yarn build && mv dist/src/main.js dist/src/production-server.js && mv dist/src/main.js.map dist/src/production-server.js.map",
    "build:staging": "yarn build && mv dist/src/main.js dist/src/staging-server.js && mv dist/src/main.js.map dist/src/staging-server.js.map",
    "start": "yarn prebuild && NODE_ENV=development nest start -p ./tsconfig.json",
    "start:dev": "yarn prebuild && NODE_ENV=development nest start --watch -p ./tsconfig.json",
    "start:debug": "yarn prebuild && NODE_ENV=development nest start --debug --watch -p ./tsconfig.json",
    "start:staging": "NODE_ENV=staging DEPLOYMENT=staging pm2 start dist/src/staging-server.js && pm2 save",
    "start:prod": "NODE_ENV=production DEPLOYMENT=production pm2 start dist/src/production-server.js && pm2 save",
    "stop:staging": "pm2 delete staging-server && pm2 save",
    "stop:prod": "pm2 delete production-server && pm2 save",
    "test": "NODE_ENV=test jest",
    "test:coverage": "yarn test --collectCoverage",
    "test:watch": "yarn test --watch",
    "test:report": "yarn test:coverage; open test-reports/html/index.html",
    "release:staging": "yarn build:staging && yarn test && yarn package:staging",
    "release:prod": "yarn build:prod && yarn test && yarn sentry:release && yarn package:prod",
    "sentry:release": "gulp releaseToSentry",
    "sentry:sorucemaps": "gulp sourceMapsToSentry",
    "sentry:login": "sentry-cli login",
    "package:prod": "zip -r prod-release.zip README.md tsconfig.json package.json src config",
    "package:staging": "zip -r staging-release.zip README.md tsconfig.json package.json src config",
    "staging": "yarn install && yarn build:staging && yarn start:staging",
    "prod": "yarn install && yarn build:prod && yarn start:prod"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "@sentry/integrations": "6.2.0",
    "@sentry/node": "6.2.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.2",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.2.6",
    "firebase-admin": "^9.5.0",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "nest-raven": "^7.1.0",
    "node-config-ts": "^3.1.0",
    "node-ical": "^0.12.8",
    "nodemailer": "^6.4.18",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "purify-ts": "^0.16.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@sentry/cli": "^1.71.0",
    "@testdeck/jest": "^0.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.14.31",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "chalk": "^4.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "jest-stare": "^2.2.1",
    "supertest": "^6.1.3",
